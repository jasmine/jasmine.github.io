{
  "source": {
    "include": [ "_versions/edge/lib/" ],
    "includePattern": ".+\\.js$"
  },
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc"]
  },
  "opts": {
    "destination": "_api/edge",
    "encoding": "utf8",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown",
    "_jsdoc_plugins/see_tutorials"
  ],
  "templates": {
    "cleverLinks": true,
    "default": {
      "outputSourceFiles": false,
      "useLongnameInNav": true,
      "layoutFile": "_layouts/jsdoc-layout.tmpl"
    }
  }
}
