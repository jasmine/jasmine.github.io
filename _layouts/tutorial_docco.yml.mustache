---
# This file was generated by locco from {{ &src_path }}. Do not edit it directly!
layout: tutorial
redirect_from:
{{#redirectFrom}}
    - "{{ &path }}"
{{/redirectFrom}}
include_docco: true
file_name: "{{ &filename }}"
---
<table class="docco" cellspacing="0" cellpadding="0">
  <tbody>
  {{#sections}}
  <tr id="section-{{ id }}">
    <td class="docs">
      <div class="pilwrap">
        <a class="pilcrow" href="#section-{{ id }}">&#182;</a>
      </div>
      <div>
        {{{ docs }}}
      </div>
    </td>
    <td class="code">
      {% highlight javascript %}{{{ code }}} {% endhighlight %}
    </td>
  </tr>
  {{/sections}}
  </tbody>
</table>
